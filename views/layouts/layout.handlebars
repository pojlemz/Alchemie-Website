    <div class="modal fn-license-agreement-modal css-license-agreement-modal fn-modal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title css-float-center css-modal-title-center">License Agreement</h5>
